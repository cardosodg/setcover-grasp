strict graph G {
size="8.5,11;"
ratio = "expand;"
fixedsize="true;"
overlap="scale;"
node[shape=circle,width=.12,hight=.12,fontsize=12]
edge[fontsize=12]

0 [color=black];
1 [color=blue];
