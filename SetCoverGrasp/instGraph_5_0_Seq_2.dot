3 [color=blue];
4 [color=black];

0 -- 1;
0 -- 2;
0 -- 3;
0 -- 1 -- 4;
1 -- 0 -- 2;
1 -- 0 -- 3;
1 -- 4;
2 -- 0 -- 3;
2 -- 0 -- 1 -- 4;
3 -- 0 -- 1 -- 4;

}
